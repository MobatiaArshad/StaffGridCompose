<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="Theme.StaffGridCompose" parent="android:Theme.Material.Light.NoActionBar">
        <item name="colorPrimary">@color/theme_navi_blue</item>
        <item name="android:statusBarColor">@color/white</item>
    </style>



    <style name="Theme.SplashScreen.MySplash" parent="Theme.SplashScreen">
        <item name="windowSplashScreenBackground">#01113D</item>
        <item name="windowSplashScreenAnimatedIcon">@drawable/ic_launcher_foreground</item>
        <item name="windowSplashScreenAnimationDuration">3000</item>
        <item name="postSplashScreenTheme">@style/Theme.StaffGridCompose</item>
    </style>
</resources>